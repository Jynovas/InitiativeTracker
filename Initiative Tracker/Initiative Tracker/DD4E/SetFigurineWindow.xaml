        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="209,241,0,0" VerticalAlignment="Top" Width="75" Click="CancelButton_Click"/>
        <Button x:Name="OkayButton" Content="Okay" HorizontalAlignment="Left" Margin="129,241,0,0" VerticalAlignment="Top" Width="75" Click="OkayButton_Click"/>
