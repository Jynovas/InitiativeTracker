<Window x:Class="Initiative_Tracker.DD4E.HealCombatantWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="HealCombatantWindow" Height="300" Width="300">
    <Grid>
        <Label x:Name="PromptLabel" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="52" Width="274"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="209,241,0,0" VerticalAlignment="Top" Width="75" Click="CancelButton_Click"/>
        <Button x:Name="HealButton" Content="Heal" HorizontalAlignment="Left" Margin="129,241,0,0" VerticalAlignment="Top" Width="75" Click="HealButton_Click"/>
        <TextBox x:Name="HealthTextBox" HorizontalAlignment="Left" Height="23" Margin="163,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
