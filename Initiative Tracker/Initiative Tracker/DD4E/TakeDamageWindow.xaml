<Window x:Class="Initiative_Tracker.DD4E.TakeDamageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TakeDamageWindow" Height="300" Width="300">
    <Grid>
        <ListBox x:Name="DamageTypeList" HorizontalAlignment="Left" Height="164" Margin="10,72,0,0" VerticalAlignment="Top" Width="135" SelectionMode="Multiple"/>
        <Label Content="Damge Types:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <Label x:Name="PromptLabel" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="274"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="209,241,0,0" VerticalAlignment="Top" Width="75" Click="CancelButton_Click"/>
        <Button x:Name="DamageButton" Content="Damage" HorizontalAlignment="Left" Margin="129,241,0,0" VerticalAlignment="Top" Width="75" Click="DamageButton_Click"/>
        <TextBox x:Name="DamageAmountTextBox" HorizontalAlignment="Left" Height="23" Margin="164,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Damage Amount:" HorizontalAlignment="Left" Margin="164,41,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
